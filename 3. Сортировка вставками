using System;

public class InsertionSort {
    public static void Sort(int[] array) {
        // Начинаем со второго элемента (индекс 1)
        // Первый элемент считается отсортированной частью
        for (int i = 1; i < array.Length; i++) {
            // Запоминаем текущий элемент, который будем вставлять
            int key = array[i];
            
            // Индекс для перемещения по отсортированной части
            int j = i - 1;

            // Сдвигаем элементы отсортированной части, которые больше key,
            // чтобы освободить место для вставки
            // Двигаемся справа налево по отсортированной части
            while (j >= 0 && array[j] > key) {
                // Сдвигаем элемент вправо
                array[j + 1] = array[j];
                j--; // Переходим к следующему элементу слева
            }
            
            // Вставляем key на правильную позицию
            array[j + 1] = key;
            
            // Теперь элементы array[0..i] отсортированы
        }
    }

    public static void Main() {
        int[] array = {12, 11, 13, 5, 6};
        Console.WriteLine("Исходный массив: " + string.Join(" ", array));
        Sort(array);
        Console.WriteLine("Отсортированный массив: " + string.Join(" ", array));
    }
}
